@model IEnumerable<examProject21.Models.ExamClass>

@{
    ViewData["Title"] = "Index";
}

<h1>Get an estimate of your renovation subsidy</h1>
<hr/>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.energy)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.unit)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.water)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.mUnit)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@try
{
    foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.energy)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.unit)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.water)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.mUnit)
            </td>
         
        </tr>
    }
}
catch (Exception e)
            {
                <td style="font-size:20px;">Error: @e.Message </td>
            }
    </tbody>
</table>
